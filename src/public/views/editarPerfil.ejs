<html lang="br">
  <head>
    <meta charset="UTF-8" />
    <title>Editar Perfil - TaskFull</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />

    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
  

    <link
      href="../estilos/geral.css"
      rel="stylesheet"
      type="text/css"
      media="all"
    />
    <link
      href="../estilos/dashboard.css"
      rel="stylesheet"
      type="text/css"
      media="all"
    />
  </head>


  <body id="produtos">
    <div id="tudo">
      <header>
        <strong><h1>TaskFull</h1></strong>
     
        <nav>
          <a href="listas" id="in">Listas</a>
          <a href="perfil" id="qs">Perfil</a>
          <button onclick="deslogar()" id="deslogar">Deslogar</button>
      </nav>
      
        <p id="busca"></p>
      </header>
      <main>

       
        <span id="cod_lista" hidden></span>
        <span id="span_msg" hidden></span>
        <form class="add_lista_form" action="" method="post">
       
            <strong><p>Nome</p></strong>
            <input
              type="text"
              id="nomeUsuario"
              required
              placeholder="Digite seu nome"
            />
            <br />
            <strong><p>Email</p></strong>
            <input type="email" id="emailUsuario" name="categoria" required placeholder="Digite a categoria da Lista"></input>
            <br />

            <strong><p>Senha</p></strong>
            <input
              type="text"
              id="passwordUsuario"
              required
              placeholder="Digite seu novo password"
            />
            <br />
        </form>
        <button class="btn btn-primary" id="btn-editar">Editar</button>
        <br>
        <p><a href="perfil">Voltar</a></p>


      <section id="root"></section>



</main>
    

      <footer id="rodape">
        <p>Baseado no <a href="#">BMMotores do Maujor.</a></p>
      </footer>
    </div>

    <script type="text/javascript" src="./views/editarPerfil.js"></script>
  </body>
</html>
